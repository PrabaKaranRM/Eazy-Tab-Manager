{
    "manifest_version": 2,
    "name": "Eazy Tab Manager",
    "version": "1.0",
    "description": "Manage your browser tabs with Eazy",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },
    "permissions": ["tabs",
    "activeTab",
    "storage",
    "https://*/*",
    "http://*/*",
    "downloads"],
    "browser_action": {
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
          },
      "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }
  